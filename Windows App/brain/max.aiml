<?xml version="1.0" encoding="utf-8"?>
<aiml>
	<category>
		<pattern>mom</pattern>
		<template>
			<bot name="mother"/>.
		</template>
	</category>
	<category>
		<pattern>state</pattern>
		<template>
			<bot name="state"/>
		</template>
	</category>
