<?xml version="1.0" encoding="utf-8" ?>
<aiml>
	<category>
		<pattern>I am *</pattern>
		<template>
			Hello <set name = "username"> <star/>!
			</set>
		</template>
	</category>
	<category>
		<pattern>my name is *</pattern>
		<template>
			Hello <set name = "username"> <star/>!
			</set>
		</template>
	</category>
	<category>
		<pattern>what is my name</pattern>
		<template>
			Your name is  <get name = "username"/>.
		</template>
	</category>
</aiml>